    "preview": "vite preview"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.1.0",
    "eslint": "^8.27.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.52.0",
    "vite": "^3.2.3"
  }
}
