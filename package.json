{
  "name": "sigmaker",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base=/sigmaker/",
    "buildtest": "vite build --base=/test/",
    "deploy2prod": "pnpm build && rsync -av --dry-run --exclude .DS_Store dist/ squidward:/srv/caddy/site/files.ink.fish/sigmaker/",
